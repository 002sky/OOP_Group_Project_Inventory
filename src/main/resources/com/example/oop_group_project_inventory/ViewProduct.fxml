<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<HBox alignment="CENTER" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.oop_group_project_inventory.ViewProduct">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <VBox alignment="CENTER" spacing="30.0">
        <Label text="Product List" styleClass="h3"/>
        <ComboBox fx:id="CbViewProductType"></ComboBox>
        <TextField fx:id="searchField" promptText="Search by ID"/>
        <Pane>
            <children>
                <TableView fx:id="GroceryTableView" visible="false"/>

                <TableView fx:id="ElectronicTableView" visible="false"/>

                <TableView fx:id="ClothingTableView" visible="false"/>
            </children>
        </Pane>
    </VBox>
</HBox>

